{
  "name": "mpss-front",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/rc-slider": "^8.6.5",
    "axios": "^0.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "start": "start-storybook -p 6006 -s public",
    "build": "build-storybook -s public -o build/storybook",
    "tsc-dry-run": "tsc --noEmit",
    "lint:js": "eslint --ext .js,.jsx,.tsx,.ts --fix .",
    "lint:css": "stylelint --fix 'src/**/*.css'",
    "format": "prettier --write '**/*.{json,md}'",
    "svgo": "svgo -f src/icons --config=src/config/svgo.config.json",
    "lint": "concurrently --raw 'yarn run lint:js' 'yarn run lint:css' 'yarn run format'"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not ie 11"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/config/setupTests.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run tsc-dry-run && npm run build-storybook && CI=true npm run test"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.css": [
      "stylelint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.svg": [
      "svgo --config=src/config/svgo.config.json",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/runtime": "^7.4.5",
    "@storybook/addon-a11y": "^5.1.9",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-knobs": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-storyshots": "^5.1.9",
    "@storybook/addon-storyshots-puppeteer": "^5.1.9",
    "@storybook/addon-viewport": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/react": "^5.1.9",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "@types/react-modal": "^3.8.2",
    "@types/react-select": "^3.0.0",
    "@types/storybook__addon-knobs": "^5.0.3",
    "@types/storybook__react": "^4.0.2",
    "@types/throttle-debounce": "^1.1.1",
    "@types/yup": "^0.26.22",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "@typescript-eslint/typescript-estree": "^1.11.0",
    "@yankovsky/final-form-focus": "^1.1.4",
    "@yankovsky/whitepaper-bem": "^2.1.0",
    "babel-loader": "8.0.5",
    "bem-cn": "^3.0.1",
    "concurrently": "^4.1.1",
    "date-fns": "^1.30.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "final-form": "^4.18.2",
    "final-form-arrays": "^3.0.1",
    "husky": "^2.4.1",
    "jest-enzyme": "^7.0.2",
    "lint-staged": "^8.2.1",
    "lodash": "^4.17.15",
    "lorem-ipsum": "^2.0.3",
    "postcss-calc": "^7.0.1",
    "postcss-color-function": "^4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.1.2",
    "postcss-simple-vars": "^5.0.2",
    "prettier": "^1.18.2",
    "rc-slider": "^8.6.13",
    "react-app-rewire-postcss": "^3.0.2",
    "react-app-rewired": "^2.1.3",
    "react-final-form": "^6.3.0",
    "react-final-form-arrays": "^3.1.0",
    "react-modal": "^3.9.1",
    "react-number-format": "^4.1.0",
    "react-select": "^3.0.4",
    "react-test-renderer": "^16.8.6",
    "redux-act": "^1.7.7",
    "redux-devtools": "^3.5.0",
    "redux-devtools-extension": "^2.13.8",
    "require-context.macro": "^1.0.4",
    "reselect": "^4.0.0",
    "shave": "^2.5.4",
    "storybook-addon-themes": "^5.2.0",
    "stylelint": "^10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-prettier": "^1.1.1",
    "svgo": "^1.3.0",
    "throttle-debounce": "^2.1.0",
    "ts-essentials": "^3.0.0",
    "typescript": "^3.5.3",
    "yup": "^0.27.0"
  }
}
